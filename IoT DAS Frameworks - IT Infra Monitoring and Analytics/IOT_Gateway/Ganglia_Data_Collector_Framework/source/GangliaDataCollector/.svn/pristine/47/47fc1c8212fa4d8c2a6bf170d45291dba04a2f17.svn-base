log4j.rootLogger=INFO, FileLogger


log4j.appender.FileLogger=org.apache.log4j.RollingFileAppender
log4j.appender.FileLogger.File=log/DogWatch.log
log4j.appender.FileLogger.MaxFileSize=5MB
log4j.appender.FileLogger.MaxBackupIndex=25

log4j.appender.FileLogger.layout=org.apache.log4j.PatternLayout
# Print the date in ISO 8601 format
log4j.appender.FileLogger.layout.ConversionPattern=%d{ISO8601} [%p,%c{1},%t] %m%n

